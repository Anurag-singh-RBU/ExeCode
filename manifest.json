{
    "manifest_version": 3,
    "name": "Form404",
    "version": "1.0",
    "description": "Auto fill Google Forms automatically",
  
    "action": {
      "default_popup": "popup.html"
    },
  
    "permissions": ["storage"],
  
    "host_permissions": [
      "https://docs.google.com/forms/*"
    ],
  
    "content_scripts": [
      {
        "matches": ["https://docs.google.com/forms/*"],
        "js": ["content.js"]
      }
    ]
}
  